@article{Jensen2017,
abstract = {Zoos e aqu{\'{a}}rios visam alcan{\c{c}}ar um impacto duradouro na conscientiza{\c{c}}{\~{a}}o do p{\'{u}}blico p{\'{u}}blico sobre a biodiversidade, seu valor e os passos que podem tomar para conserv{\'{a}}-lo. Aqui, avaliamos o impacto educacional a longo prazo das visitas aos zool{\'{o}}gicos e aqu{\'{a}}rios sobre a compreens{\~{a}}o da biodiversidade e o conhecimento das a{\c{c}}{\~{o}}es para ajudar a proteger a biodiversidade. Um m{\'{i}}nimo de dois anos ap{\'{o}}s a conclus{\~{a}}o de um levantamento de medidas repetidas antes e depois de visitar um zool{\'{o}}gico ou aqu{\'{a}}rio, os mesmos participantes foram convidados a participar de uma pesquisa on-line de acompanhamento. Apesar do pequeno n{\'{u}}mero de entrevistados (n = 161), nosso estudo ainda pode representar as melhores evid{\^{e}}ncias quantitativas dispon{\'{i}}veis relativas ao impacto educacional a longo prazo das visitas ao aqu{\'{a}}rio e ao jardim zool{\'{o}}gico. Descobrimos que as melhorias na compreens{\~{a}}o da biodiversidade dos entrevistados de pr{\'{e}} e p{\'{o}}s-visita se estabilizaram, permanecendo inalterado na pesquisa de acompanhamento. Em contrapartida, o melhor conhecimento das a{\c{c}}{\~{o}}es para ajudar a proteger a biodiversidade de pr{\'{e}} para p{\'{o}}s-visita mostrou uma melhoria adicional da pesquisa p{\'{o}}s-visita ao atraso p{\'{o}}s-visita. Esses resultados sugerem que os efeitos positivos imediatos de uma visita ao jardim zool{\'{o}}gico ou ao aqu{\'{a}}rio sobre os resultados de aprendizagem relacionados {\`{a}} biodiversidade podem ser duradouros e at{\'{e}} mesmo ajudar a estabelecer as bases para novas melhorias durante um longo per{\'{i}}odo de tempo ap{\'{o}}s a visita.},
author = {Jensen, Eric A. and Moss, Andrew and Gusset, Markus},
doi = {10.1002/zoo.21372},
issn = {10982361},
journal = {Zoo Biology},
keywords = {aquarium,biodiversity,education,impact,visit,zoo},
title = {{Quantifying long-term impact of zoo and aquarium visits on biodiversity-related learning outcomes}},
year = {2017}
}
@misc{Jiang2016,
abstract = {Scientific research articles can provide rich insights into practitioners' viewpoints around contentious policy making. Although much attention has been paid to hydropower development in the literature, few of them gathered systematic data and performed a large-scale review of scientific articles. In this study, we employed a topic modeling based bibliometric analysis to quantitatively evaluate global scientific literature of hydropower, with a time frame from 1994 to 2013. We analyzed 1726 scholarly articles highly related to hydropower, to discover the research development, current trends and intellectual structure of hydropower literature. Common bibliometric indicators show that hydropower research publications sustain a rapid growth rate, English is the dominant language, and the hotspots of hydropower research can be concluded as "fish", "species", "climate", "emission", "lake", "sediment", "Turkey", etc. We established a 29-topic model to describe the intellectual structure of the 1726 articles, and employed cluster analysis and trend analysis to process the derived topics. We find that post construction issues of hydropower are more attractive for scholars than construction technology itself, and an interdisciplinary trend of hydropower research is emerging. The methodology reported in this study is expected to gain traction as a methodological strategy for energy research reviews and subsequently to promote energy policy making.},
author = {Jiang, Hanchen and Qiang, Maoshan and Lin, Peng},
booktitle = {Renewable and Sustainable Energy Reviews},
doi = {10.1016/j.rser.2015.12.194},
issn = {18790690},
keywords = {Bibliometrics,Hydropower,Renewable energy,Research trends,Topic modeling},
title = {{A topic modeling based bibliometric exploration of hydropower research}},
year = {2016}
}
