journal = {Advances in Nutrition},
keywords = {Food Supply -- International Aspects,Food Supply -- Management,Food insecurity,Food security scales,Indicators,Measurement,anth8400,evaluation,food insecurity,indicators,meta-analysis,methods,poverty,qualitative,qualitative data},
number = {February},
