@article{Alaofe2017,
abstract = {Background:Evidence on effectiveness of women's empowerment (WE) to reduce undernutrition is limited in sub-Sahara Africa, and few studies incorporate multidimensional measures of WE.Objective:To examine whether a WE status, in sum and across leadership, decision-making, mobility, economic security, male involvement in housework, and nonfamily group domains, is associated with women and their children nutritional status in Kalal{\'{e}} district of northern Benin.Methods:Data were obtained from the 2014 Solar Market Garden baseline study: 767 paired reproductive-age women aged 15 to 49 years and children 6 to 59 months old. Exploratory principal component (cross-validate with confirmatory) factor analysis was first conducted to identify the structure of empowerment. Then, using a new survey-based index, regression analysis was conducted to examine associations between WE measures and maternal dietary diversity score (DDS) and body mass index (BMI), as well as their child's DDS, height-for-age z score (HAZ), weig...},
