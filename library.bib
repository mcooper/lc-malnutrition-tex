year = {2013}
}
@incollection{Peterman2014,
abstract = {This paper reviews existing microeconomic empirical literature on gender differences in use, access, and adoption of nonland agricultural inputs in developing countries. This review focuses on four key areas: (1) technological resources, (2) natural resources, (3) human resources, and (4) social and political capital. In general, there has been more empirical research on inorganic fertilizer, seed varieties, extension services, and group membership than on tools and mechanization, life-cycle effects, and political participation. Across input areas, generally men have higher input measures than women; however, this finding is often sensitive to the use of models that control for other background factors, as well as the type of gender indicator implemented in the analysis. We find few studies that meet our inclusion criteria outside Sub-Saharan Africa. Finally, future directions, opportunities, and recommendations for microeconomic gender analysis of nonland agricultural inputs are discussed.},
author = {Peterman, Amber and Behrman, Julia A. and Quisumbing, Agnes R.},
booktitle = {Gender in Agriculture: Closing the Knowledge Gap},
doi = {10.1007/978-94-017-8616-4_7},
isbn = {9789401786164},
keywords = {Access to farm inputs,Agriculture,Assets,Gender,Women},
publisher = {Food and Agricultural Organization of the United Nations},
title = {{A review of empirical evidence on gender differences in nonland agricultural inputs, technology, and services in developing countries}},
year = {2014}
}
@techreport{WBG2016,
abstract = {What Africa's drought responses teach us about climate change hotspots The impact of drought has been felt acutely this season over southern Africa, as El Ni{\~{n}}o hit hard. Perhaps this is what we might expect under future climate change conditions and so we had better learn how to prepare for it: more frequent years that record less rainfall than usual, along with the associated crippling impact on livelihoods and the economy. This is particularly the case in semi-arid regions of Africa and Asia. These climate change hotspots are highly dynamic systems that already experience harsh climates, adverse environmental change and a relative lack of natural resources. People here are often further marginalised by high levels of poverty, inequality and rapidly changing socio-economic, governance and development contexts. This requires an effective response. In northern Namibia and eastern Botswana, research is already underway into what's currently working and not working in relation to managing climate impacts. A major regional project is seeking ways to},
author = {WBG},
booktitle = {World Bank Group},
doi = {10.1111/1467-8322.12302},
isbn = {9781742082851},
issn = {14678322},
pmid = {20333943},
title = {{World Bank Group Climate Change Action plan 2016-2020}},
year = {2016}
}
@article{Brown2012a,
abstract = {The U.S. Agency for International Development (USAID)'s Famine Early$\backslash$nWarning System Network (FEWS NET) provides monitoring and early warning$\backslash$nsupport to decision makers responsible for responding to food insecurity$\backslash$nemergencies on three continents. FEWS NET uses satellite remote sensing$\backslash$nand ground observations of rainfall and vegetation in order to provide$\backslash$ninformation on drought, floods, and other extreme weather events$\backslash$nto decision makers. Previous research has presented results from$\backslash$na professional review questionnaire with FEWS NET expert end-users$\backslash$nwhose focus was to elicit Earth observation requirements. The review$\backslash$nprovided FEWS NET operational requirements and assessed the usefulness$\backslash$nof additional remote sensing data. We analyzed 1342 food security$\backslash$nupdate reports from FEWS NET. The reports consider the biophysical,$\backslash$nsocioeconomic, and contextual influences on the food security in$\backslash$n17 countries in Africa from 2000 to 2009. The objective was to evaluate$\backslash$nthe use of remote sensing information in comparison with other important$\backslash$nfactors in the evaluation of food security crises. The results show$\backslash$nthat all 17 countries use rainfall information, agricultural production$\backslash$nstatistics, food prices, and food access parameters in their analysis$\backslash$nof food security problems. The reports display large-scale patterns$\backslash$nthat are strongly related to history of the FEWS NET program in each$\backslash$ncountry. We found that rainfall data were used 84{\%} of the time,$\backslash$nremote sensing of vegetation 28{\%} of the time, and gridded crop models$\backslash$n10{\%} of the time, reflecting the length of use of each product in$\backslash$nthe regions. More investment is needed in training personnel on remote$\backslash$nsensing products to improve use of data products throughout the FEWS$\backslash$nNET system. (C) 2012 Society of Photo-Optical Instrumentation Engineers$\backslash$n(SPIE). $\backslash${\{}[$\backslash${\}}DOI: 10.1117/1.JRS.6.063511]},
author = {Brown, Molly E. and Brickley, Elizabeth B.},
doi = {10.1117/1.JRS.6.063511},
issn = {19313195},
journal = {Journal of Applied Remote Sensing},
keywords = {12,17,2011,2012,23,accepted for,drought monitoring,food security,imaging systems,paper 11119 received jul,publication jan,published online mar,remote sensing,revised manuscript received dec,satellites},
title = {{Evaluating the use of remote sensing data in the U.S. Agency for International Development Famine Early Warning Systems Network}},
year = {2012}
