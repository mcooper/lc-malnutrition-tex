keywords = {AD 1975,AD 1990,AD 2000,AD 2014,Earth,Earth Observation,Global Human Settlement Layer project,Human Settlements,Image resolution,Landsat,Landsat imagery global archive,Monitoring,Remote sensing,Satellites,Sociology,Urban areas,Urbanization,disaster risk,disasters,early warning models,global built up environment,global environmental change processes,global fine scale population density grids,global human settlement layer,hazards,information layers,international framework agreements,natural hazards,urbanization statistics generation},
