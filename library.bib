author = {Burgert, Clara R},
booktitle = {DHS Spatial Analysis Reports No. 9 },
publisher = {ICF International },
title = {{Spatial interpolation with Demographic and Health Survey data: Key considerations }},
url = {http://dhsprogram.com/pubs/pdf/SAR9/SAR9.pdf},
