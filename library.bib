author = {Garenne, Michel},
booktitle = {DHS Comparative Reports No. 27 },
publisher = {ICF Macro },
title = {{Trends in nutritional status of adult women in sub-Saharan Africa }},
url = {http://dhsprogram.com/pubs/pdf/CR27/CR27.pdf},
year = {2011}
}
