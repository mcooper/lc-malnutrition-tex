author = {Al-Nsour, Mohannad and Arbaji, Ali},
booktitle = {DHS Working Papers No. 115 },
publisher = {ICF International },
title = {{Obesity and related factors among Jordanian women of reproductive age based on three DHS surveys, 2002-2012 }},
