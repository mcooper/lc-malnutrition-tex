publisher = {ICF International },
title = {{An inventory of alcohol-related questions in the Demographic and Health Surveys and an analysis of alcohol use and unsafe sex in sub-Saharan Africa    }},
url = {http://dhsprogram.com/pubs/pdf/AS53/AS53.pdf},
year = {2015}
}
