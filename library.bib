booktitle = {DHS Methodological Reports No. 8 },
publisher = {ICF International },
title = {{Results of the pilot survey of an experimental module of sexual activity questions }},
url = {http://dhsprogram.com/pubs/pdf/MR8/MR8.pdf},
year = {2014}
}
