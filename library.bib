@article{Smith2015,
abstract = {Summary Background Anthropogenic declines of animal pollinators and the associated effects on human nutrition are of growing concern. We quantified the nutritional and health outcomes associated with decreased intake of pollinator-dependent foods for populations around the world. Methods We assembled a database of supplies of 224 types of food in 156 countries. We quantified nutrient composition and pollinator dependence of foods to estimate the size of possible reductions in micronutrient and food intakes for different national populations, while keeping calorie intake constant with replacement by staple foods. We estimated pollinator-decline-dependent changes in micronutrient-deficient populations using population-weighted estimated average requirements and the cutpoint method. We estimated disease burdens of non-communicable, communicable, and malnutrition-related diseases with the Global Burden of Disease 2010 comparative risk assessment framework. Findings Assuming complete removal of pollinators, 71 million (95{\%} uncertainty interval 41-262) people in low-income countries could become newly deficient in Vitamin A, and an additional 2{\textperiodcentered}2 billion (1{\textperiodcentered}2-2{\textperiodcentered}5) already consuming below the average requirement would have further declines in Vitamin A supplies. Corresponding estimates for folate were 173 million (134-225) and 1{\textperiodcentered}23 billion (1{\textperiodcentered}12-1{\textperiodcentered}33). A 100{\%} decline in pollinator services could reduce global fruit supplies by 22{\textperiodcentered}9{\%} (19{\textperiodcentered}5-26{\textperiodcentered}1), vegetables by 16{\textperiodcentered}3{\%} (15{\textperiodcentered}1-17{\textperiodcentered}7), and nuts and seeds by 22{\textperiodcentered}1{\%} (17{\textperiodcentered}7-26{\textperiodcentered}4), with significant heterogeneity by country. In sum, these dietary changes could increase global deaths yearly from non-communicable and malnutrition-related diseases by 1{\textperiodcentered}42 million (1{\textperiodcentered}38-1{\textperiodcentered}48) and disability-adjusted life-years (DALYs) by 27{\textperiodcentered}0 million (25{\textperiodcentered}8-29{\textperiodcentered}1), an increase of 2{\textperiodcentered}7{\%} for deaths and 1{\textperiodcentered}1{\%} for DALYs. A 50{\%} loss of pollination services would be associated with 700 000 additional annual deaths and 13{\textperiodcentered}2 million DALYs. Interpretation Declines in animal pollinators could cause significant global health burdens from both non-communicable diseases and micronutrient deficiencies. Funding Winslow Foundation, Bill {\&} Melinda Gates Foundation.},
author = {Smith, Matthew R. and Singh, Gitanjali M. and Mozaffarian, Dariush and Myers, Samuel S.},
doi = {10.1016/S0140-6736(15)61085-6},
isbn = {1474-547X (Electronic)$\backslash$r0140-6736 (Linking)},
issn = {1474547X},
journal = {The Lancet},
number = {10007},
pages = {1964--1972},
pmid = {26188748},
title = {{Effects of decreases of animal pollinators on human nutrition and global health: A modelling analysis}},
volume = {386},
year = {2015}
}
