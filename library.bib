title = {{Domestic violence and women's autonomy in developing countries: Theory and evidence}},
year = {2011}
}
@incollection{Altman2005,
abstract = {Continuous variables are often converted to categorical variables by grouping values. For descriptive purposes, this can be useful but problems can arise when this is done as part of a data analysis. This article discusses relevant issues with categorization of explanatory variables in regression. Effects of categorization are discussed, such as loss of information, power, and efficiency. This is considered first for categorizing into two groups and then for categorizing into several groups. Rather than group according to observed values with several variables, one might group according to a weighted sum of the variables–a prognostic index.},
author = {Altman, Douglas G.},
booktitle = {Encyclopedia of Biostatistics},
