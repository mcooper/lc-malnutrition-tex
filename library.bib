journal = {Conservation and Society},
keywords = {and e,assessing the relationship between,benjamin palmer fry,giles atkinson,graham wallace,human,j,katherine homewood,marcus,milner-gulland,rowcliffe,susana mourato,thew agarwala},
number = {4},
pages = {437},
title = {{Assessing the Relationship Between Human Well-being and Ecosystem Services: A Review of Frameworks}},
