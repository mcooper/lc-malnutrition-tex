author = {Annim, Samuel Kobina and Awusabo-Asare, Kofi and Amo-Adjei, Joshua},
booktitle = {DHS Working Papers No. 98 },
publisher = {ICF International },
title = {{Household nucleation, dependency and child health outcomes in Ghana }},
