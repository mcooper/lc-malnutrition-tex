journal = {Conservation Letters},
keywords = {Acid rain,Bibliometrics,Culturomics,DDT,Elephant,Media,Policy},
number = {2},
pages = {98--106},
title = {{Multiple pathways to conservation success}},
volume = {6},
year = {2013}
}
