doi = {10.1038/540030a},
issn = {0028-0836},
journal = {Nature},
title = {{A new global research agenda for food}},
year = {2016}
}
@article{Funk2014,
abstract = {Recent analyses of Internet search behaviour conclude that the public's$\backslash$ninterest in environmental issues is falling (McCallum and Bury, Biodiv$\backslash$nConserv 22:1355-1367, 2013). Ficetola (Biodiv Conserv 22:2983-2988,$\backslash$n2013) argued that the nature of the underpinning data processing may$\backslash$ncreate an artificially declining trend, even when the absolute number of$\backslash$nsearches increases and public interest is growing. These findings are$\backslash$nhighly relevant for applied conservation strategies and the public media$\backslash$nhave quickly picked the message of the alarming fading interest$\backslash$nworldwide, the possibility of devastating repercussions and calls for$\backslash$nrapid responses in conservation communication. We challenge both$\backslash$nanalysis by evaluating Internet searches of English and non-English$\backslash$nspeaking users. The inclusion of information on the linguistic$\backslash$nbackground reveals a much more differentiated picture, with some$\backslash$ncultures displaying an increasing interest and others a decreasing$\backslash$ninterest. These analyses allow a better understanding of the importance$\backslash$nof global-local viewpoints, cultural knowledge and cultural differences$\backslash$non the interpretation of underpinning human interest from Internet$\backslash$nsearch patterns. Despite methodological problems limiting the utility of$\backslash$nsummary data provided by search engines, they can offer powerful$\backslash$ninformation when applied spatially and temporally restricted and$\backslash$nanalysed alongside suitable benchmark indicators. We discuss that due$\backslash$nconsideration of methodological caveats is essential to inform the$\backslash$ngeneral public about the relevance for conservation without triggering$\backslash$nsensationalist or over-generalizing conclusions. Conservation$\backslash$ncommunication needs considering that Internet search engines do not$\backslash$nnecessarily mirror the interest of many people who are essential for the$\backslash$nconservation of biodiversity.},
author = {Funk, Stephan M. and Rusowsky, Daniela},
doi = {10.1007/s10531-014-0767-6},
isbn = {0960-3115},
issn = {15729710},
journal = {Biodiversity and Conservation},
keywords = {Biodiversity,Communication,Conservation,Google trends,Public opinion,Time series analysis},
number = {12},
pages = {3101--3112},
title = {{The importance of cultural knowledge and scale for analysing internet search data as a proxy for public interest toward the environment}},
volume = {23},
year = {2014}
}
