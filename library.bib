file = {:C$\backslash$:/Users/matt/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Assogbadjo et al. - 2012 - Biodiversity and socioeconomic factors supporting farmers' choice of wild edible trees in the agroforestry sy.pdf:pdf},
isbn = {1389-9341, 1389-9341},
issn = {13899341},
journal = {Forest Policy and Economics},
keywords = {Biodiversity,Guineo-congolian zone,Socioeconomic factors,Sudanian zone,Sudano-guinean zone,Traditional agroforestry},
number = {1},
pages = {41--49},
publisher = {Elsevier B.V.},
title = {{Biodiversity and socioeconomic factors supporting farmers' choice of wild edible trees in the agroforestry systems of Benin (West Africa)}},
