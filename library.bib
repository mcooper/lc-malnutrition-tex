keywords = {Sahel,Senegal,desertification,drought,indicators,land degradation,land-cover changes,remote sensing},
pages = {129--148},
title = {{Monitoring land-cover changes in semi-arid regions: remote sensing data and field observations in the Ferlo, Senegal}},
