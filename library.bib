@misc{Shackleton2004,
abstract = {We review and synthesize recent South African work that examines the$\backslash$nrole and importance of non-timber forest products (NTFPs) in the daily$\backslash$nlives of rural people in South Africa. The most commonly used such$\backslash$nproducts are wild spinaches, fuelwood, wooden utensils edible fruits,$\backslash$ngrass hand-brushes, and twig hand-brushes, used by 85{\%} or more of$\backslash$nhouseholds. More than half the households investigated also make use of$\backslash$nedible insects, wood for construction, bushmeat, wild honey and reeds$\backslash$nfor weaving. Individual households may exploit dozens of animal and$\backslash$nplant species. The range in annual, direct-use values is large, from$\backslash$nless than 81000 per household per year to over R12 000. The value to$\backslash$nrural households is manifest through a daily net function which$\backslash$nrepresents a cost saving to the families involved and to the state, as$\backslash$nwell as through an emergency net, which serves as an insurance in times$\backslash$nof misfortune, such as drought, disease, and unexpected economic$\backslash$nhardship. The emergency net function has hardly been quantified in South$\backslash$nAfrica and internationally. Ad hoc trade in NTFPs is a common emergency$\backslash$nnet, which in some instances evolves into a permanent way of life.$\backslash$nFinancial returns from trade are variable, depending on resource type$\backslash$nand hours worked, but are typically low. Despite the small cash incomes$\backslash$nfrom trade, they provide an important contribution that complement the$\backslash$ndiverse livelihood strategies within a household, especially for the$\backslash$npoorer sectors of rural society. Moreover, there are non-financial$\backslash$nbenefits of NTFP trade that are commonly overlooked.},
author = {Shackleton, Charlie and Shackleton, Sheona},
booktitle = {South African Journal of Science},
isbn = {0038-2353},
issn = {00382353},
title = {{The importance of non-timber forest products in rural livelihood security and as safety nets: A review of evidence from South Africa}},
year = {2004}
}
@misc{Franklin2012,
abstract = {One in seven American households experience food insecurity at times during the year, lack of money and other resources hinder their ability to maintain consistent access to nutritious foods. Low-income, ethnic minority, and female-headed households exhibit the greatest risk for food insecurity, which often results in higher prevalence of diet-related disease. The food insecurity-obesity paradox is one that researchers have explored to understand the factors that influence food insecurity and its impact on weight change. The aim of this inquiry was to explore new evidence in associations of food insecurity and obesity in youth, adult, and elderly populations. A literature search of publication databases was conducted, using various criteria to identify relevant articles. Among 65 results, 19 studies conducted since 2005 were selected for review. Overall, the review confirmed that food insecurity and obesity continue to be strongly and positively associated in women. Growing evidence of this association was found in adolescents; but among children, results remain mixed. Few studies supported a linear relationship between food insecurity and weight outcomes, as suggested by an earlier review. New mediators were revealed (gender, marital status, stressors, and food stamp participation) that alter the association; in fact, newer studies suggest that food stamp participation may exacerbate obesity outcomes. Continued examination through longitudinal studies, development of tools to distinguish acute and chronic food insecurity, and greater inclusion of food security measurement tools in regional and local studies are warranted.},
author = {Franklin, Brandi and Jones, Ashley and Love, Dejuan and Puckett, Stephane and Macklin, Justin and White-Means, Shelley},
booktitle = {Journal of Community Health},
doi = {10.1007/s10900-011-9420-4},
issn = {00945145},
keywords = {Children,Food insecurity,Mediators,Obesity,Women},
title = {{Exploring mediators of food insecurity and obesity: A review of recent literature}},
year = {2012}
}
