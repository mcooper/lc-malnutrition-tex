@article{Black2010,
abstract = {Background: Up-to-date information on the causes of child deaths is crucial to guide global efforts to improve child survival. We report new estimates for 2008 of the major causes of death in children younger than 5 years. Methods: We used multicause proportionate mortality models to estimate deaths in neonates aged 0-27 days and children aged 1-59 months, and selected single-cause disease models and analysis of vital registration data when available to estimate causes of child deaths. New data from China and India permitted national data to be used for these countries instead of predictions based on global statistical models, as was done previously. We estimated proportional causes of death for 193 countries, and by application of these proportions to the country-specific mortality rates in children younger than 5 years and birth rates, the numbers of deaths by cause were calculated for countries, regions, and the world. Findings: Of the estimated 8{\textperiodcentered}795 million deaths in children younger than 5 years worldwide in 2008, infectious diseases caused 68{\%} (5{\textperiodcentered}970 million), with the largest percentages due to pneumonia (18{\%}, 1{\textperiodcentered}575 million, uncertainty range [UR] 1{\textperiodcentered}046 million-1{\textperiodcentered}874 million), diarrhoea (15{\%}, 1{\textperiodcentered}336 million, 0{\textperiodcentered}822 million-2{\textperiodcentered}004 million), and malaria (8{\%}, 0{\textperiodcentered}732 million, 0{\textperiodcentered}601 million-0{\textperiodcentered}851 million). 41{\%} (3{\textperiodcentered}575 million) of deaths occurred in neonates, and the most important single causes were preterm birth complications (12{\%}, 1{\textperiodcentered}033 million, UR 0{\textperiodcentered}717 million-1{\textperiodcentered}216 million), birth asphyxia (9{\%}, 0{\textperiodcentered}814 million, 0{\textperiodcentered}563 million-0{\textperiodcentered}997 million), sepsis (6{\%}, 0{\textperiodcentered}521 million, 0{\textperiodcentered}356 million-0{\textperiodcentered}735 million), and pneumonia (4{\%}, 0{\textperiodcentered}386 million, 0{\textperiodcentered}264 million-0{\textperiodcentered}545 million). 49{\%} (4{\textperiodcentered}294 million) of child deaths occurred in five countries: India, Nigeria, Democratic Republic of the Congo, Pakistan, and China. Interpretation: These country-specific estimates of the major causes of child deaths should help to focus national programmes and donor assistance. Achievement of Millennium Development Goal 4, to reduce child mortality by two-thirds, is only possible if the high numbers of deaths are addressed by maternal, newborn, and child health interventions. Funding: WHO, UNICEF, and Bill {\&} Melinda Gates Foundation. {\textcopyright} 2010 Elsevier Ltd. All rights reserved.},
author = {Black, Robert E. and Cousens, Simon and Johnson, Hope L. and Lawn, Joy E. and Rudan, Igor and Bassani, Diego G. and Jha, Prabhat and Campbell, Harry and Walker, Christa Fischer and Cibulskis, Richard and Eisele, Thomas and Liu, Li and Mathers, Colin},
doi = {10.1016/S0140-6736(10)60549-1},
isbn = {1474-547X (Electronic)$\backslash$r0140-6736 (Linking)},
issn = {01406736},
journal = {The Lancet},
pmid = {20466419},
title = {{Global, regional, and national causes of child mortality in 2008: a systematic analysis}},
year = {2010}
}
