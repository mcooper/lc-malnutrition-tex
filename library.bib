journal = {Demography},
keywords = {Intimate partner violence,Recession,Relationship quality},
title = {{Intimate Partner Violence in the Great Recession}},
year = {2016}
}
@article{Castella2013,
abstract = {This study investigates four decades of socio-economic and environmental change in a shifting cultivation landscape in the northern uplands of Laos. Historical changes in land cover and land use were analyzed using a chronological series of remote sensing data. Impacts of landscape change on local livelihoods were investigated in seven villages through interviews with various stakeholders. The study reveals that the complex mosaics of agriculture and forest patches observed in the study area have long constituted key assets for the resilience of local livelihood systems in the face of environmental and socio-economic risks. However, over the past 20 years, a process of segregating agricultural and forest spaces has increased the vulnerability of local land users. This process is a direct outcome of policies aimed at increasing national forest cover, eradicating shifting cultivation and fostering the emergence of more intensive and commercial agricultural practices. We argue that agriculture-forest segregation should be buffered in such a way that a diversity of livelihood opportunities and economic development pathways can be maintained.},
author = {Castella, Jean Christophe and Lestrelin, Guillaume and Hett, Cornelia and Bourgoin, Jeremy and Fitriana, Yulia Rahma and Heinimann, Andreas and Pfund, Jean Laurent},
doi = {10.1007/s10745-012-9538-8},
file = {:C$\backslash$:/Users/matt/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Castella et al. - 2013 - Effects of Landscape Segregation on Livelihood Vulnerability Moving From Extensive Shifting Cultivation to Rota.pdf:pdf},
isbn = {0300-7839},
issn = {03007839},
journal = {Human Ecology},
keywords = {Ecosystem services,Land sparing,Laos,Livelihood vulnerability,Multifunctional landscapes,Shifting cultivation,Southeast Asia},
number = {1},
pages = {63--76},
title = {{Effects of Landscape Segregation on Livelihood Vulnerability: Moving From Extensive Shifting Cultivation to Rotational Agriculture and Natural Forests in Northern Laos}},
volume = {41},
year = {2013}
}
